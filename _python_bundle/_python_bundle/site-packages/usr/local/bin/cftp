#!/home/user/.local/share/python-for-android/build/other_builds/hostpython3/desktop/hostpython3/native-build/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Twisted==0.0.0','console_scripts','cftp'
__requires__ = 'Twisted==0.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Twisted==0.0.0', 'console_scripts', 'cftp')()
    )
